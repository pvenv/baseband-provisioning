<rpc message-id="Configure Routers" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <edit-config>
    <target>
      <running/>
    </target>
    <config>
	<ManagedElement xmlns="urn:com:ericsson:ecim:ComTop">
        <managedElementId>1</managedElementId>
         <Equipment>
          <equipmentId>1</equipmentId>
            <FieldReplaceableUnit>
              <fieldReplaceableUnitId>1</fieldReplaceableUnitId>
              <administrativeState>UNLOCKED</administrativeState>
              <TnPort>
                <tnPortId>TN_B</tnPortId>
              </TnPort>
                    </FieldReplaceableUnit>
         </Equipment>
             <Transport>
          <transportId>1</transportId>
          <EthernetPort xmlns="urn:com:ericsson:ecim:RtnL2EthernetPort">
            <!-- Example ethernetPortId: TN_B -->
            <ethernetPortId>1</ethernetPortId>
            <administrativeState>UNLOCKED</administrativeState>
            <!-- Example admOperatingMode: 10G_FULL -->
            <admOperatingMode>ANY</admOperatingMode>
            <!-- Example autoNegEnable: false -->
            <autoNegEnable>true</autoNegEnable>
            <!-- Reference to physical TN port, must match the TnPort configured in the site equipment file. -->
            <!-- Example encapsulation: ManagedElement=1,Equipment=1,FieldReplaceableUnit=1,TnPort=TN_B -->
            <encapsulation>ManagedElement=1,Equipment=1,FieldReplaceableUnit=1,TnPort=TN_B</encapsulation>
          </EthernetPort>
          <VlanPort xmlns="urn:com:ericsson:ecim:RtnL2VlanPort">
            <vlanPortId>OAM</vlanPortId>
            <!-- Example enacapsulation: ManagedElement=1,Transport=1,EthernetPort=TN_B -->
            <encapsulation>ManagedElement=1,Transport=1,EthernetPort=1</encapsulation>
            <!-- Example isTagged: true -->
            <isTagged>true</isTagged>
            <!-- Example vlanId: 810 -->
            <vlanId>2666</vlanId>
            <userLabel>VLAN Interface for OAM connection</userLabel>
          </VlanPort>
          <Router xmlns="urn:com:ericsson:ecim:RtnL3Router">
            <routerId>OAM</routerId>
            <InterfaceIPv4 xmlns="urn:com:ericsson:ecim:RtnL3InterfaceIPv4">
              <interfaceIPv4Id>OAM</interfaceIPv4Id>
              <AddressIPv4 xmlns="urn:com:ericsson:ecim:RtnL3AddressIPv4">
                <addressIPv4Id>OAM</addressIPv4Id>
                <!-- Example address: 1.2.3.4/28 -->
                <address>10.91.6.101/30</address>
              </AddressIPv4>
              <encapsulation>ManagedElement=1,Transport=1,VlanPort=OAM</encapsulation>
              <userLabel>IP Interface for OAM connection</userLabel>
            </InterfaceIPv4>
            <RouteTableIPv4Static xmlns="urn:com:ericsson:ecim:RtnRoutesStaticRouteIPv4">
              <routeTableIPv4StaticId>Routes_OAM</routeTableIPv4StaticId>
              <!-- One or more routes, default or specific, to be able to reach the OAM network. -->
              <Dst>
                <dstId>1</dstId>
                <NextHop>
                  <nextHopId>Routes_OAM</nextHopId>
                  <!-- Example address: 1.2.3.4 -->
                  <address>10.91.6.102</address>
                </NextHop>
                <!-- Example dst: 0.0.0.0/0 -->
                <dst>0.0.0.0/0</dst>
              </Dst>
            </RouteTableIPv4Static>
          </Router>
        
       </Transport>
      </ManagedElement>
    </config>
  </edit-config>
</rpc>
]]>]]>

<rpc message-id="Close Session" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <close-session/>
</rpc>

