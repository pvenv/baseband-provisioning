<rpc message-id="Configure System Functions" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <edit-config>
    <target>
      <running/>
    </target>
    <config>
      <ManagedElement xmlns="urn:com:ericsson:ecim:ComTop">
        <managedElementId>1</managedElementId>        
        <SystemFunctions>
          <systemFunctionsId>1</systemFunctionsId>
          <SecM xmlns="urn:com:ericsson:ecim:ComSecM">
            <secMId>1</secMId>
            <UserManagement>
              <userManagementId>1</userManagementId>
              <!-- One or more target types for TBAC. -->
              <!-- Example targetTypes: TCU03_81,TCU -->
			  <!--
              <targetType>%targetType1%</targetType>
              <targetType>%targetType2%</targetType>
			  -->
              <UserIdentity xmlns="urn:com:ericsson:ecim:RcsUser" >
                <userIdentityId>1</userIdentityId>
                <!-- At least the first maintenance user should be defined during AI since it's not possible to do later. -->
                <MaintenanceUser>
                  <maintenanceUserId>1</maintenanceUserId>
                  <!-- User name and password to use when logging in as a maintenance user over SSH. -->
                  <!-- Can be ommitted if only maintenance user over TLS will be used. -->
                  <userName>rbs</userName>
                  <password>
                    <cleartext/>
                    <password>rbs</password>
                  </password>                             
                  <!-- Full subject name of a certificate to use when logging in as a maintenance user over TLS. -->
                  <!-- Can be committed if only maintenance user over SSH will be used. -->
                  <!-- Example subjectName: CN=Maintuser -->
                  <!-- <subjectName>%subjectName_maintUser1%</subjectName> -->
                </MaintenanceUser>
              </UserIdentity>
            </UserManagement>
          </SecM>
		  	 <SysM xmlns="urn:com:ericsson:ecim:ComSysM">
 <sysMId>1</sysMId>
		  
          </SysM>
        </SystemFunctions>
      </ManagedElement>
    </config>
  </edit-config>
</rpc>
