<!-- OAM Access Point configuration and set networkManagedElementId of the ME -->
<!-- NOTE: MUST be set after transport configuration -->
<hello xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <capabilities>
    <capability>urn:ietf:params:netconf:base:1.0</capability>
  </capabilities>
</hello>
]]>]]>

<rpc message-id="Configure OAM Access Point and set networkManagedElementId of the ME" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <edit-config>
    <target>
      <running/>
    </target>
    <config>
      <ManagedElement xmlns="urn:com:ericsson:ecim:ComTop">
        <managedElementId>1</managedElementId>
        <!-- A unique identity of the Managed Element (ME) must be set in attribute networkManagedElementId. 
           This is to be able to connect the ME MIB to the overall network MIB, and to be able to uniquely 
           identify the ME in for instance, alarms, performance management data, and logs.
       
           NOTE: setting of networkManagedElementId MUST be in the last config RPC!!!
         -->
        <networkManagedElementId>BTS_56_00618_G</networkManagedElementId>
        <SystemFunctions>
          <systemFunctionsId>1</systemFunctionsId>
          <SysM xmlns="urn:com:ericsson:ecim:ComSysM">
            <sysMId>1</sysMId>
            <!-- 
                OamAccessPoint (OAM Access Point configuration)
                  - Insert reference to Transport IP address used for OAM traffic (ipv4address)
                  NOTE: MUST be set after transport configuration
                 -->
            <OamAccessPoint xmlns="urn:com:ericsson:ecim:RcsOamAccessPoint">
              <oamAccessPointId>1</oamAccessPointId>
              <ipv4address>ManagedElement=1,Transport=1,Router=OAM,InterfaceIPv4=OAM,AddressIPv4=OAM</ipv4address>
            </OamAccessPoint>
          </SysM>
        </SystemFunctions>
      </ManagedElement>
    </config>
  </edit-config>
</rpc>
]]>]]>

<rpc message-id="Close Session" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <close-session/>
</rpc>
