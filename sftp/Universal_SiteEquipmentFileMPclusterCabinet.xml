<rpc message-id="Create Cabinet 6601" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <edit-config>
    <config>
      <ManagedElement>
        <managedElementId>1</managedElementId>
        <Equipment>
          <equipmentId>1</equipmentId>
          <FieldReplaceableUnit>
            <fieldReplaceableUnitId>1</fieldReplaceableUnitId>
            <positionRef>ManagedElement=1,Equipment=1,Cabinet=1</positionRef>
        <!--    <EcPort>
              <ecPortId>1</ecPortId>
              <hubPosition>X</hubPosition> 
            </EcPort>  -->
          </FieldReplaceableUnit>
          <Cabinet>
             <cabinetId>1</cabinetId>
             <climateSystem>STANDARD</climateSystem>
             <climateRegulationSystem>NOT_APPLICABLE</climateRegulationSystem>
             <productData>
                <productName>RBS6601</productName>
                <productNumber>0</productNumber>
                <productRevision>0</productRevision>
                <productionDate>00W01</productionDate>
                <serialNumber>0</serialNumber>
             </productData>
          </Cabinet>
		  <EcBus>                                                                                                                                                                                                                                                                     
            <ecBusId>1</ecBusId>                                                                                                                                                                                                                                                
            <ecBusConnectorRef>ManagedElement=1,Equipment=1,FieldReplaceableUnit=1</ecBusConnectorRef>                                                                                                                                                                       
          </EcBus>
		  
		  <!-- Adding EC-bus for cabinet FieldReplaceableUnit=1 and positionRef to Cabinet -->
		  
		  <FieldReplaceableUnit>                                                                                                                                                                                                                                                      
            <fieldReplaceableUnitId>1</fieldReplaceableUnitId>                                                                                                                                                                                                               
            <positionRef>ManagedElement=1,Equipment=1,Cabinet=1</positionRef>                                                                                                                                                                                                   
            <EcPort>                                                                                                                                                                                                                                                            
              <ecPortId>1</ecPortId>                                                                                                                                                                                                                                      
              <hubPosition>X</hubPosition>                                                                                                                                                                                                                               
              <ecBusRef>ManagedElement=1,Equipment=1,EcBus=1</ecBusRef>                                                                                                                                                                                                   
            </EcPort>                                                                                                                                                                                                                                                           
          </FieldReplaceableUnit>

          <!-- Adding SUP-1 for cabinet RBS6601 -->
	      <FieldReplaceableUnit>
             <fieldReplaceableUnitId>SUP-1</fieldReplaceableUnitId>
             <administrativeState>UNLOCKED</administrativeState>
             <positionRef>ManagedElement=1,Equipment=1,Cabinet=1</positionRef>
             <EcPort>
               <ecPortId>1</ecPortId>
               <hubPosition>NA</hubPosition>
               <ecBusRef>ManagedElement=1,Equipment=1,EcBus=1</ecBusRef>
             </EcPort>
                 <!-- Add or remove alarmports and set site alarmslogan here-->
             <AlarmPort>
               <alarmPortId>1</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>2</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>3</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>4</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>5</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>6</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>7</alarmPortId>
             </AlarmPort>
             <AlarmPort>
               <alarmPortId>8</alarmPortId>
             </AlarmPort>
          </FieldReplaceableUnit>
        </Equipment>
        <EquipmentSupportFunction>
          <equipmentSupportFunctionId>1</equipmentSupportFunctionId>
          <supportSystemControl>true</supportSystemControl>
          
          
          
          <Climate xmlns="urn:com:ericsson:ecim:ResClimate">
             <climateId>1</climateId>
             <controlDomainRef>ManagedElement=1,Equipment=1,Cabinet=1</controlDomainRef>
             <climateControlMode>NORMAL</climateControlMode>
          </Climate>
        </EquipmentSupportFunction>
      </ManagedElement>
    </config>
  </edit-config>
</rpc>

]]>]]>

